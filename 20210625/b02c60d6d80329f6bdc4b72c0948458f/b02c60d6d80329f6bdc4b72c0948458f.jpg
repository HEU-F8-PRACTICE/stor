function Main()
  SN = gg.choice({
    "1.攻击速度上色",
    "2.攻击速度上色还原",
    "3.普通加强",
    "4.加强还原",
    "退出脚本"
  }, nil, "KOF97")
  if SN == 1 then
    a()
  end
  if SN == 2 then
    b()
  end
  if SN == 3 then
    c()
  end
  if SN == 4 then
    d()
  end
  
  if SN == 5 then
    Exit()
  end
  XGCK = -1
end

